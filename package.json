{
    "name": "churning-search",
    "version": "0.7.9",
    "description": "Churning subreddit search using the Pushshift API",
    "repository": "https://github.com/garettg/churning-search.git",
    "homepage": "https://churning.io",
    "author": "garettg",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "rm -r ./dist && parcel src/index.html",
        "build": "rm -r ./dist && parcel build src/index.html",
        "rebuild": "rm -r ./dist && parcel build src/index.html && cp CNAME ./dist",
        "dist": "rm -r ./dist && parcel build --public-url ./ src/index.html && cp CNAME ./dist && export $(cat .env | xargs) && gh-pages --dist ./dist --repo https://$GH_TOKEN@github.com/garettg/churning-search.git"
    },
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.3.2",
        "@parcel/packager-xml": "^2.3.2",
        "@parcel/transformer-webmanifest": "^2.3.2",
        "@parcel/transformer-xml": "^2.3.2",
        "autoprefixer": "^10.4.2",
        "cssnano": "^5.0.17",
        "gh-pages": "^3.2.3",
        "parcel": "^2.3.2",
        "postcss": "^8.4.7",
        "yarn-audit-fix": "^9.1.2"
    },
    "dependencies": {
        "@tailwindcss/forms": "~0.4.1",
        "@types/react": "~17.0.48",
        "@types/react-dom": "~17.0.17",
        "date-fns": "~2.29.1",
        "lz-string": "~1.4.4",
        "prop-types": "~15.8.1",
        "react": "~17.0.2",
        "react-clipboard.js": "^2.0.16",
        "react-date-range": "~1.4.0",
        "react-dom": "~17.0.2",
        "react-ga": "~3.3.1",
        "react-markdown": "~5.0.3",
        "react-toastify": "^8.2.0",
        "remark-gfm": "~1.0.0",
        "tailwindcss": "^2.2.19",
        "time-ago": "~0.2.1",
        "typescript": "^4.5.5",
        "underscore": "~1.13.4"
    }
}
